#!/bin/sh

case "$1" in
	start)
		echo -n "Starting validateurtestapp"
		validateurtestapp
		echo "done"
		;;
	stop)
		echo -n "Stopping validateurtestapp: "
		killapp validateurtestapp
		echo "done"
		;;
	restart)
		"$0" stop
		sleep 1
		"$0" start
		;;
	*)
		echo "Usage: $0 {start|stop|restart}"
		;;
esac

