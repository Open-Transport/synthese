			<xs:documentation source="IV.4.24">Enumération des types de diffusion d’une messagerie passager</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Differe">
				<xs:annotation>
					<xs:documentation source="IV.4.24">Envoi programmé</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Repete">
				<xs:annotation>
					<xs:documentation source="IV.4.24">Envoi programmé avec répétition</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DriverMessageTextType">
		<xs:annotation>
			<xs:documentation source="IV.4.25">Texte du message conducteur</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Line" maxOccurs="3">
				<xs:annotation>
					<xs:documentation source="IV.4.25">Ligne du texte d’un message</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="27"/>
						<xs:minLength value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PassengerMessageTextType">
		<xs:annotation>
			<xs:documentation source="IV.4.26">Texte du message passager</xs:documentation>
		</xs:annotation>
		<xs:sequence>
