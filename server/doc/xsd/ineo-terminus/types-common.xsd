<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--III.2 Formatage des requêtes et réponses-->
	<xs:complexType name="Request">
		<xs:sequence>
			<xs:element name="ID" type="RequestIdentifierType">
				<xs:annotation>
					<xs:documentation>Identifiant unique de la requête</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestTimeStamp" type="DateTime">
				<xs:annotation>
					<xs:documentation>Horodatage de la demande</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestorRef" type="RequestorRefType">
				<xs:annotation>
					<xs:documentation>Elément permettant d’identifier le demandeur de manière unique</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Response">
		<xs:sequence>
			<xs:element name="ID" type="ResponseIdentifierType">
				<xs:annotation>
					<xs:documentation>Identifiant unique de la réponse</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestID" type="RequestIdentifierType">
				<xs:annotation>
					<xs:documentation>Identifiant unique de la requête</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponseTimeStamp" type="DateTime">
				<xs:annotation>
					<xs:documentation>Horodatage de la demande</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponseRef" type="ResponseRefType">
				<xs:annotation>
					<xs:documentation>Elément permettant d’identifier le fournisseur de la réponse de manière unique</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ErrorTypeEnum">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ProtocolError"/>
			<xs:enumeration value="ApplicationError"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ErrorIDEnum">
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1">
				<xs:annotation>
