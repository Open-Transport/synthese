				<xs:restriction base="PpdsMessageTextType">
					<xs:sequence>
						<xs:element name="Line" nillable="0">
							<xs:annotation>
								<xs:documentation source="IV.4.28">Texte d’un message</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1"/>
