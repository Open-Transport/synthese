								<xs:documentation>Tout le réseau</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Lines" type="LineListType">
							<xs:annotation>
								<xs:documentation>Liste de lignes</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Bivs" type="BivListType">
							<xs:annotation>
								<xs:documentation>Liste de bornes</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
