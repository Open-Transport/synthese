Import ('s3env')

senv = s3env.Clone ()

prj = CppBinProject (senv, 's3-db-client', s3env['S3VERSION'], 
                     source_files = senv.Glob ('main.cpp'),
                     dependencies = ['s3-db.shlib', 's3-util-iostreams.shlib', 'boost-program-options.shlib'])
senv.SConsProject (prj)

